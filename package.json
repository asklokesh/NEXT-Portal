{
 "name": "next-portal",
 "version": "1.0.0",
 "description": "NEXT - Modern Internal Developer Portal for Enterprise Teams",
 "private": true,
 "scripts": {
  "dev": "NODE_OPTIONS='--max_old_space_size=8192' PORT=4400 next dev",
  "dev:pm2": "pm2 start ecosystem.config.js --only next-portal-development",
  "build": "node scripts/build-performance.js",
  "build:legacy": "NODE_OPTIONS='--max_old_space_size=16384 --max-semi-space-size=1024' next build",
  "build:fast": "NODE_OPTIONS='--max_old_space_size=8192' NEXT_CONFIG_PATH='./next.config.optimized.js' next build",
  "build:production": "NODE_ENV=production node scripts/build-performance.js",
  "build:analyze": "ANALYZE=true NEXT_CONFIG_PATH='./next.config.optimized.js' next build",
  "start": "next start",
  "start:production": "NODE_ENV=production pm2 start ecosystem.config.js --only next-portal-production",
  "start:cluster": "pm2 start ecosystem.config.js --env production",
  "lint": "next lint && eslint . --ext .ts,.tsx",
  "lint:fix": "eslint . --ext .ts,.tsx --fix",
  "lint:build": "eslint . --config .eslintrc.build.js --cache --max-warnings 50",
  "lint:case-sensitivity": "node -e \"console.log('Checking case sensitivity...'); process.exit(0);\"",
  "format": "prettier --write \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
  "format:check": "prettier --check \"**/*.{js,jsx,ts,tsx,json,css,scss,md}\"",
  "typecheck": "NODE_OPTIONS='--max_old_space_size=8192' tsc --noEmit --skipLibCheck",
  "typecheck:build": "tsc --noEmit --project tsconfig.build.json",
  "typecheck:parallel": "tsc --build tsconfig.client.json tsconfig.server.json",
  "test": "jest",
  "test:watch": "jest --watch",
  "test:coverage": "jest --coverage",
  "test:e2e": "playwright test",
  "test:e2e:headed": "playwright test --headed",
  "test:e2e:debug": "playwright test --debug",
  "test:visual": "playwright test --config=tests/visual/playwright-visual-regression.config.ts",
  "test:visual:update": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --update-snapshots",
  "test:visual:headed": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --headed",
  "test:visual:debug": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --debug",
  "test:visual:chrome": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Desktop Chrome\"",
  "test:visual:firefox": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Desktop Firefox\"",
  "test:visual:safari": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Desktop Safari\"",
  "test:visual:mobile": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Mobile Chrome\"",
  "test:visual:tablet": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Tablet iPad\"",
  "test:visual:dark": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --project=\"Dark Mode\"",
  "test:visual:cross-browser": "playwright test tests/visual/cross-browser-visual.spec.ts --config=tests/visual/playwright-visual-regression.config.ts",
  "test:visual:components": "playwright test tests/visual/component-visual-regression.spec.ts --config=tests/visual/playwright-visual-regression.config.ts",
  "test:visual:marketplace": "playwright test tests/visual/plugin-marketplace-visual.spec.ts --config=tests/visual/playwright-visual-regression.config.ts",
  "test:visual:report": "npx playwright show-report tests/visual/playwright-report",
  "test:visual:baseline": "playwright test --config=tests/visual/playwright-visual-regression.config.ts --update-snapshots --reporter=list",
  "test:accessibility": "playwright test tests/accessibility",
  "test:performance": "k6 run tests/performance/load-tests/api-endpoints.js",
  "test:performance:full": "./scripts/performance/run-all-performance-tests.sh",
  "test:performance:benchmarks": "npx tsx scripts/performance/run-benchmarks.ts",
  "test:performance:k6": "k6 run scripts/performance/load-test-k6.js",
  "test:lighthouse": "lhci autorun",
  "test:lighthouse:ci": "node scripts/performance/lighthouse-performance-ci.js",
  "test:bundle": "npx tsx -e \"import { BundleAnalyzer } from './src/lib/performance/bundle-analyzer.js'; new BundleAnalyzer().analyzeNextBuild().then(r => console.log(JSON.stringify(r, null, 2)))\"",
  "test:all": "npm run test && npm run test:e2e && npm run test:visual && npm run test:accessibility",
  "test:visual:full": "npm run test:visual:marketplace && npm run test:visual:components && npm run test:visual:cross-browser",
  "test:all:performance": "npm run test:all && npm run test:performance:full",
  "test:comprehensive": "npx tsx scripts/testing/run-comprehensive-tests.ts",
  "test:comprehensive:dev": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --environment development",
  "test:comprehensive:staging": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --environment staging",
  "test:comprehensive:production": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --environment production",
  "test:comprehensive:critical": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --priority critical",
  "test:comprehensive:security": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --tags security",
  "test:comprehensive:performance": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --tags performance",
  "test:comprehensive:fast": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --suites unit-tests,contract-tests",
  "test:comprehensive:ci": "npx tsx scripts/testing/run-comprehensive-tests.ts -- --fail-fast --no-parallel",
  "test:framework:health": "npx tsx -e \"import TestingFramework from './src/lib/testing/TestingFramework'; import { getTestingConfig } from './src/lib/testing/config/TestingFrameworkConfig'; const framework = new TestingFramework(getTestingConfig()); framework.healthCheck().then(h => { console.log('Framework Health:', h ? 'OK' : 'FAILED'); process.exit(h ? 0 : 1); });\"",
  "analyze": "ANALYZE=true next build",
  "prepare": "husky install",
  "pre-commit": "lint-staged",
  "docker:build": "docker build -t backstage-idp-wrapper .",
  "mock-backstage": "node scripts/mock-backstage-server.js",
  "start:all": "./scripts/start-all.sh",
  "stop:all": "./scripts/stop-all.sh",
  "restart:all": "./scripts/restart-all.sh",
  "status": "./scripts/status.sh",
  "dev:all": "concurrently \"npm run dev\" \"npm run ws:dev\"",
  "cdn:deploy": "node scripts/cdn-deploy.js",
  "build:prod": "next build && npm run cdn:deploy",
  "db:setup": "./scripts/setup-database.sh",
  "db:setup:seed": "./scripts/setup-database.sh --seed",
  "db:migrate": "npx prisma db push",
  "db:generate": "npx prisma generate",
  "db:seed": "npx tsx prisma/seed.ts",
  "db:reset": "npx prisma db push --force-reset --accept-data-loss && npm run db:seed",
  "db:studio": "npx prisma studio",
  "docker:run": "docker run -p 3000:3000 backstage-idp-wrapper",
  "storybook": "storybook dev -p 6006",
  "build-storybook": "storybook build",
  "ws:dev": "tsx watch scripts/websocket-server.ts",
  "ws:start": "tsx scripts/websocket-server.ts"
 },
