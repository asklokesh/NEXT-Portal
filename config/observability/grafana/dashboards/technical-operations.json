    "refresh": "30s",
    "version": 3,
    "panels": [
      {
        "id": 1,
        "title": "System Resource Overview",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "CPU Usage",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 70},
                {"color": "red", "value": 90}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "saas_idp_cpu_usage_percent{component=\"system\"}",
            "legendFormat": "{{instance}} System CPU",
            "refId": "A"
          },
          {
            "expr": "saas_idp_cpu_usage_percent{component=\"process\"}",
            "legendFormat": "{{instance}} Process CPU",
            "refId": "B"
          }
        ]
      },
