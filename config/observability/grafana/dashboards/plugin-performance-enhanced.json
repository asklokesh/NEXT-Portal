{
  "dashboard": {
    "id": null,
    "title": "SaaS IDP - Plugin Performance & Lifecycle",
    "tags": ["saas-idp", "plugins", "performance", "lifecycle"],
    "style": "dark",
    "timezone": "browser",
    "editable": true,
    "graphTooltip": 1,
    "time": {
      "from": "now-2h",
      "to": "now"
    },
    "timepicker": {
      "refresh_intervals": ["10s", "30s", "1m", "5m", "15m", "30m", "1h"],
      "time_options": ["5m", "15m", "1h", "6h", "12h", "24h", "2d", "7d"]
    },
    "refresh": "30s",
    "version": 4,
    "panels": [
      {
        "id": 1,
        "title": "Plugin Health Summary",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 0},
        "collapsed": false
      },
      {
        "id": 2,
        "title": "Plugin Health Status",
        "type": "piechart",
        "gridPos": {"h": 8, "w": 8, "x": 0, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [
              {
                "options": {
                  "0": {"color": "red", "text": "Unhealthy"},
                  "1": {"color": "green", "text": "Healthy"},
                  "2": {"color": "yellow", "text": "Degraded"}
                },
                "type": "value"
              }
            ],
            "unit": "short"
          }
        },
        "options": {
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          },
          "pieType": "pie",
          "tooltip": {
            "mode": "single",
            "sort": "none"
          },
          "legend": {
            "displayMode": "table",
            "placement": "right",
            "values": ["value", "percent"]
          }
        },
        "targets": [
          {
            "expr": "count by (status) (label_replace(saas_idp_plugin_health_status, \"status\", \"Healthy\", \"__name__\", \".*\") * on () (saas_idp_plugin_health_status == 1) or label_replace(saas_idp_plugin_health_status, \"status\", \"Unhealthy\", \"__name__\", \".*\") * on () (saas_idp_plugin_health_status == 0))",
            "legendFormat": "{{status}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 3,
        "title": "Critical Plugin Issues",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 8, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 3}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "targets": [
          {
            "expr": "count(saas_idp_plugin_health_status == 0)",
            "legendFormat": "Critical Issues",
            "refId": "A"
          }
        ]
      },
      {
        "id": 4,
        "title": "Plugin Install Success Rate",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 12, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "red", "value": null},
                {"color": "yellow", "value": 90},
                {"color": "green", "value": 95}
              ]
            },
            "unit": "percent",
            "min": 0,
            "max": 100
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "targets": [
          {
            "expr": "(sum(rate(saas_idp_plugin_installs_total{status=\"success\"}[1h])) / sum(rate(saas_idp_plugin_installs_total[1h]))) * 100",
            "legendFormat": "Install Success Rate",
            "refId": "A"
          }
        ]
      },
      {
        "id": 5,
        "title": "Active Plugin Versions",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 16, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "value",
          "graphMode": "area",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "targets": [
          {
            "expr": "count(group by (plugin_id, version) (saas_idp_plugin_health_status))",
            "legendFormat": "Plugin Versions",
            "refId": "A"
          }
        ]
      },
      {
        "id": 6,
        "title": "Plugin Rollbacks (24h)",
        "type": "stat",
        "gridPos": {"h": 4, "w": 4, "x": 20, "y": 1},
        "fieldConfig": {
          "defaults": {
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 1},
                {"color": "red", "value": 5}
              ]
            },
            "unit": "short"
          }
        },
        "options": {
          "colorMode": "background",
          "graphMode": "none",
          "justifyMode": "center",
          "orientation": "horizontal",
          "reduceOptions": {
            "values": false,
            "calcs": ["lastNotNull"],
            "fields": ""
          }
        },
        "targets": [
          {
            "expr": "sum(increase(saas_idp_plugin_rollbacks_total[24h]))",
            "legendFormat": "Rollbacks",
            "refId": "A"
          }
        ]
      },
      {
        "id": 7,
        "title": "Plugin Operation Performance",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 9},
        "collapsed": false
      },
      {
        "id": 8,
        "title": "Plugin Operation Latency (p95)",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 10},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 10},
                {"color": "red", "value": 30}
              ]
            },
            "unit": "s",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "histogram_quantile(0.95, sum(rate(saas_idp_plugin_operation_duration_seconds_bucket[$__rate_interval])) by (le, plugin_id, operation))",
            "legendFormat": "{{plugin_id}} - {{operation}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 9,
        "title": "Plugin Error Rates",
        "type": "timeseries",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 10},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 0.01},
                {"color": "red", "value": 0.05}
              ]
            },
            "unit": "percentunit",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "sum(rate(saas_idp_plugin_operations_total{status=\"error\"}[$__rate_interval])) by (plugin_id, operation) / sum(rate(saas_idp_plugin_operations_total[$__rate_interval])) by (plugin_id, operation)",
            "legendFormat": "{{plugin_id}} - {{operation}}",
            "refId": "A"
          }
        ]
      },
      {
        "id": 10,
        "title": "Plugin Installation & Lifecycle",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 18},
        "collapsed": false
      },
      {
        "id": 11,
        "title": "Plugin Installation Trends",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 0, "y": 19},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            },
            "unit": "ops",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean"],
            "displayMode": "list",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "sum(rate(saas_idp_plugin_installs_total{status=\"success\"}[$__rate_interval]))",
            "legendFormat": "Successful Installs",
            "refId": "A"
          },
          {
            "expr": "sum(rate(saas_idp_plugin_installs_total{status=\"error\"}[$__rate_interval]))",
            "legendFormat": "Failed Installs",
            "refId": "B"
          },
          {
            "expr": "sum(rate(saas_idp_plugin_uninstalls_total[$__rate_interval]))",
            "legendFormat": "Uninstalls",
            "refId": "C"
          },
          {
            "expr": "sum(rate(saas_idp_plugin_updates_total{status=\"success\"}[$__rate_interval]))",
            "legendFormat": "Updates",
            "refId": "D"
          }
        ]
      },
      {
        "id": 12,
        "title": "Plugin Resource Usage",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 8, "y": 19},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 10,
              "gradientMode": "none",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "line"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "yellow", "value": 100},
                {"color": "red", "value": 500}
              ]
            },
            "unit": "bytes",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "calcs": ["mean", "max"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "saas_idp_plugin_memory_usage_bytes",
            "legendFormat": "{{plugin_id}} Memory",
            "refId": "A"
          }
        ]
      },
      {
        "id": 13,
        "title": "Plugin Health Check Failures",
        "type": "timeseries",
        "gridPos": {"h": 6, "w": 8, "x": 16, "y": 19},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "drawStyle": "line",
              "lineInterpolation": "linear",
              "barAlignment": 0,
              "lineWidth": 2,
              "fillOpacity": 20,
              "gradientMode": "opacity",
              "spanNulls": false,
              "insertNulls": false,
              "showPoints": "never",
              "pointSize": 5,
              "stacking": {
                "mode": "none",
                "group": "A"
              },
              "axisPlacement": "auto",
              "axisLabel": "",
              "axisColorMode": "text",
              "scaleDistribution": {
                "type": "linear"
              },
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              },
              "thresholdsStyle": {
                "mode": "off"
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null},
                {"color": "red", "value": 1}
              ]
            },
            "unit": "short",
            "min": 0
          }
        },
        "options": {
          "legend": {
            "calcs": ["sum"],
            "displayMode": "table",
            "placement": "bottom"
          },
          "tooltip": {
            "mode": "multi",
            "sort": "desc"
          }
        },
        "targets": [
          {
            "expr": "increase(saas_idp_plugin_health_checks_total{status=\"failure\"}[$__rate_interval])",
            "legendFormat": "{{plugin_id}} Failures",
            "refId": "A"
          }
        ]
      },
      {
        "id": 14,
        "title": "Plugin Marketplace Analytics",
        "type": "row",
        "gridPos": {"h": 1, "w": 24, "x": 0, "y": 25},
        "collapsed": false
      },
      {
        "id": 15,
        "title": "Most Popular Plugins",
        "type": "table",
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 26},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "align": "auto",
              "displayMode": "auto",
              "inspect": false
            },
            "mappings": [],
            "thresholds": {
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          },
          "overrides": [
            {
              "matcher": {
                "id": "byName",
                "options": "Install Count"
              },
              "properties": [
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "custom.align",
                  "value": "center"
                }
              ]
            },
            {
              "matcher": {
                "id": "byName",
                "options": "Error Rate"
              },
              "properties": [
                {
                  "id": "unit",
                  "value": "percentunit"
                },
                {
                  "id": "custom.displayMode",
                  "value": "color-background"
                },
                {
                  "id": "thresholds",
                  "value": {
                    "steps": [
                      {"color": "green", "value": null},
                      {"color": "yellow", "value": 0.01},
                      {"color": "red", "value": 0.05}
                    ]
                  }
                }
              ]
            }
          ]
        },
        "options": {
          "showHeader": true,
          "sortBy": [
            {
              "desc": true,
              "displayName": "Install Count"
            }
          ]
        },
        "targets": [
          {
            "expr": "topk(10, sum by (plugin_id) (increase(saas_idp_plugin_installs_total{status=\"success\"}[7d])))",
            "format": "table",
            "instant": true,
            "legendFormat": "",
            "refId": "A"
          }
        ],
        "transformations": [
          {
            "id": "organize",
            "options": {
              "excludeByName": {
                "Time": true,
                "__name__": true,
                "job": true,
                "instance": true
              },
              "indexByName": {},
              "renameByName": {
                "Value": "Install Count",
                "plugin_id": "Plugin ID"
              }
            }
          }
        ]
      },
      {
        "id": 16,
        "title": "Plugin Dependency Graph",
        "type": "nodeGraph",
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 26},
        "fieldConfig": {
          "defaults": {
            "custom": {
              "hideFrom": {
                "legend": false,
                "tooltip": false,
                "vis": false
              }
            },
            "mappings": [],
            "thresholds": {
              "mode": "absolute",
              "steps": [
                {"color": "green", "value": null}
              ]
            }
          }
        },
        "options": {
          "nodes": {
            "mainStatUnit": "short",
            "secondaryStatUnit": "short",
            "arcs": [
              {
                "field": "Dependency Count",
                "color": "green"
              }
            ]
          },
          "edges": {
            "mainStatUnit": "short",
            "secondaryStatUnit": "short"
          }
        },
        "targets": [
          {
            "expr": "saas_idp_plugin_dependencies_count",
            "format": "table",
            "instant": true,
            "legendFormat": "",
            "refId": "A"
          }
        ]
      }
    ],
    "templating": {
      "list": [
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "prometheus",
          "definition": "label_values(saas_idp_plugin_health_status, plugin_id)",
          "hide": 0,
          "includeAll": true,
          "label": "Plugin",
          "multi": true,
          "name": "plugin",
          "options": [],
          "query": {
            "query": "label_values(saas_idp_plugin_health_status, plugin_id)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        },
        {
          "current": {
            "selected": false,
            "text": "All",
            "value": "$__all"
          },
          "datasource": "prometheus",
          "definition": "label_values(saas_idp_plugin_health_status, version)",
          "hide": 0,
          "includeAll": true,
          "label": "Version",
          "multi": true,
          "name": "version",
          "options": [],
          "query": {
            "query": "label_values(saas_idp_plugin_health_status, version)",
            "refId": "StandardVariableQuery"
          },
          "refresh": 1,
          "regex": "",
          "skipUrlSync": false,
          "sort": 0,
          "type": "query"
        }
      ]
    },
    "annotations": {
      "list": [
        {
          "builtIn": 1,
          "datasource": "-- Grafana --",
          "enable": true,
          "hide": true,
          "iconColor": "rgba(0, 211, 255, 1)",
          "name": "Annotations & Alerts",
          "type": "dashboard"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(saas_idp_plugin_installs_total[5m]) > 0",
          "hide": false,
          "iconColor": "blue",
          "name": "Plugin Installs",
          "showIn": 0,
          "step": "2m",
          "tagKeys": "plugin_id",
          "textFormat": "",
          "titleFormat": "Plugin Install: {{plugin_id}}"
        },
        {
          "datasource": "prometheus",
          "enable": true,
          "expr": "changes(saas_idp_plugin_rollbacks_total[5m]) > 0",
          "hide": false,
          "iconColor": "orange",
          "name": "Plugin Rollbacks",
          "showIn": 0,
          "step": "2m",
          "tagKeys": "plugin_id",
          "textFormat": "",
          "titleFormat": "Plugin Rollback: {{plugin_id}}"
        }
      ]
    }
  }
}