{
  "datacenter": "vault-dc",
  "node_name": "consul-node",
  "data_dir": "/consul/data",
  "log_level": "INFO",
  "log_json": true,
  "server": true,
  "bootstrap_expect": 3,
  "ui_config": {
    "enabled": true
  },
  "connect": {
    "enabled": true
  },
  "ports": {
    "grpc": 8502
  },
  "acl": {
    "enabled": true,
    "default_policy": "allow",
    "enable_token_persistence": true
  },
  "encrypt": "qDOPBEr+/oUqOYshTOZnShI/tUndmX1wU88aThrhXmE=",
  "verify_incoming": false,
  "verify_outgoing": false,
  "verify_server_hostname": false,
  "ca_file": "/consul/config/ssl/ca.crt",
  "cert_file": "/consul/config/ssl/consul.crt",
  "key_file": "/consul/config/ssl/consul.key",
  "auto_encrypt": {
    "allow_tls": true
  },
  "performance": {
    "raft_multiplier": 1
  },
  "leave_on_terminate": true,
  "skip_leave_on_interrupt": false
}