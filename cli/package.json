{
  "name": "@backstage-idp/cli",
  "version": "1.0.0",
  "description": "Command-line interface for Backstage Developer Portal",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "backstage-cli": "dist/cli.js",
    "bsdp": "dist/cli.js"
  },
  "scripts": {
    "build": "tsc && chmod +x dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts",
    "lint:fix": "eslint src --ext .ts --fix",
    "format": "prettier --write src/**/*.ts",
    "prepare": "npm run build",
    "pack:npm": "npm pack",
    "install:global": "npm install -g .",
    "uninstall:global": "npm uninstall -g @backstage-idp/cli"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "backstage",
    "developer-portal",
    "cli",
    "command-line",
    "devops",
    "platform-engineering"
  ],
  "author": "Backstage Team",
  "license": "Apache-2.0",
  "dependencies": {
    "@backstage-idp/sdk-typescript": "^1.0.0",
    "commander": "^11.1.0",
    "inquirer": "^9.2.12",
    "chalk": "^5.3.0",
    "ora": "^7.0.1",
    "boxen": "^7.1.1",
    "table": "^6.8.1",
    "yaml": "^2.3.4",
    "fs-extra": "^11.2.0",
    "glob": "^10.3.10",
    "semver": "^7.5.4",
    "update-notifier": "^7.0.0",
    "conf": "^12.0.0",
    "debug": "^4.3.4",
    "winston": "^3.11.0",
    "axios": "^1.6.2",
    "ws": "^8.16.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/inquirer": "^9.0.7",
    "@types/fs-extra": "^11.0.4",
    "@types/debug": "^4.1.12",
    "@types/semver": "^7.5.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "typescript": "^5.3.3",
    "ts-node": "^10.9.2",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.1.1"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backstage/backstage-idp-sdk.git",
    "directory": "cli"
  },
  "bugs": {
    "url": "https://github.com/backstage/backstage-idp-sdk/issues"
  },
  "homepage": "https://github.com/backstage/backstage-idp-sdk/tree/main/cli"
}