{
  "name": "@backstage-idp/sdk-typescript",
  "version": "1.0.0",
  "description": "TypeScript SDK for Backstage Developer Portal",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "rollup -c && npm run build:types",
    "build:types": "tsc --declaration --outDir dist --emitDeclarationOnly",
    "dev": "rollup -c -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "docs": "typedoc src/index.ts",
    "prepare": "npm run build"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "keywords": [
    "backstage",
    "developer-portal",
    "sdk",
    "typescript",
    "api-client",
    "graphql",
    "websocket"
  ],
  "author": "Backstage Team",
  "license": "Apache-2.0",
  "dependencies": {
    "axios": "^1.6.5",
    "graphql": "^16.8.1",
    "graphql-request": "^6.1.0",
    "ws": "^8.16.0",
    "eventemitter3": "^5.0.1",
    "retry": "^0.13.1",
    "circuit-breaker-js": "^0.2.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@types/retry": "^0.12.2",
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "@rollup/plugin-node-resolve": "^15.2.3",
    "@rollup/plugin-commonjs": "^25.0.7",
    "@rollup/plugin-typescript": "^11.1.6",
    "@rollup/plugin-json": "^6.1.0",
    "rollup": "^4.10.0",
    "rollup-plugin-dts": "^6.1.0",
    "typescript": "^5.3.3",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "@types/jest": "^29.5.11",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.5",
    "typedoc": "^0.25.7"
  },
  "peerDependencies": {
    "typescript": ">=4.9.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/backstage/backstage-idp-sdk.git",
    "directory": "typescript"
  },
  "bugs": {
    "url": "https://github.com/backstage/backstage-idp-sdk/issues"
  },
  "homepage": "https://github.com/backstage/backstage-idp-sdk/tree/main/typescript"
}